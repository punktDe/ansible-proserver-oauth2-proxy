{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "description": "Update oauth2-proxy version from GitHub releases",
      "managerFilePatterns": ["^defaults/main\\.yaml$"],
      "matchStrings": [
        "version:\\s*(?<currentValue>[\\d.]+)"
      ],
      "datasourceTemplate": "github-releases",
      "packageNameTemplate": "oauth2-proxy/oauth2-proxy",
      "versioningTemplate": "semver-coerced",
      "extractVersionTemplate": "^v?(?<version>.+)$"
    }
  ]
}
