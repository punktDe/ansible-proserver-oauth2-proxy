{
  "packageRules": [
    {
      "matchDepNames": ["oauth2-proxy"],
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "defaults/main.yaml"
      ],
      "matchStrings": [
        "oauth2_proxy:\\s*\\n\\s*version: \"?\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+)\"?"
      ],
      "depNameTemplate": "oauth2-proxy",
      "datasourceTemplate": "github-tags",
      "packageNameTemplate": "oauth2-proxy/oauth2-proxy",
      "versioningTemplate": "semver",
      "extractVersionTemplate": "^v(?<version>\\d+\\.\\d+\\.\\d+)$"

    }
  ]
}
